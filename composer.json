        "drupal/preview_link": "1.5.0",
        "drupal/publication_date": "^2.0@beta",
        "drupal/quick_node_clone": "1.12.0",
        "drupal/redirect": "1.5.0",
        "drupal/redis": "1.2.0",
        "drupal/response_code_condition": "1.0.0",
        "drupal/s3fs": "^3.0@alpha",
        "drupal/search_api_solr": "3.8.0",
        "drupal/simple_sitemap": "^3.6",
        "drupal/slick_views": "2.1.0",
        "drupal/social_media_links_field": "2.6.0",
        "drupal/twig_field_value": "1.2.0",
        "drupal/twig_tweak": "2.4.0",
        "drupal/views_data_export": "^1.0@beta",
        "drupal/views_slideshow": "4.6.0",
        "drupal/views_slideshow_cycle2": "1.x-dev",
        "drupal/yoast_seo": "^1.5",
        "drush/drush": "9.7.2",
        "jaypan/jquery_colorpicker": "master",
        "mnsami/composer-custom-directory-installer": "1.1.1",
        "npm-asset/slick-slider": "1.8.2",
        "oomphinc/composer-installers-extender": "1.1.2",
        "platformsh/config-reader": "2.3.1",
        "symfony/event-dispatcher": "4.3.4 as 3.4.99",
        "vlucas/phpdotenv": "2.6.1",
        "webflo/drupal-finder": "1.2.0",
        "webmozart/path-util": "2.3.0",
        "zaporylie/composer-drupal-optimizations": "1.1.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.5.5",
        "vijaycs85/drupal-quality-checker": "1.2.1",
        "webflo/drupal-core-require-dev": "8.8.5"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "bash post-install.sh"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "bash post-install.sh"
        ]
    },
    "extra": {
        "enable-patching": true,
        "patches": {
            "drupal/lightning_media": {
                "Lightning Media buld Media Upload should have option target media type[https://www.drupal.org/project/lightning_media/issues/2969541]": "https://www.drupal.org/files/issues/2018-05-08/2969541-2-lightning_bulk_upload_form.patch"
            },
            "drupal/paragraphs": {
                "Integrity constraint violation: 1048 Column 'langcode' cannot be null[https://www.drupal.org/project/paragraphs/issues/2901390]": "https://www.drupal.org/files/issues/2019-08-10/paragraphs-set_langcode_widgets-290139_updated.patch"
            },
            "drupal/disqus": {
                "Disqus migrate support is broken[https://www.drupal.org/project/disqus/issues/2879592]": "https://www.drupal.org/files/issues/disqus-migrate-support-2879592-14.patch"
            },
            "drupal/social_media_links": {
                "Mailto: Need to add Subject and Body values[https://www.drupal.org/project/social_media_links/issues/3102897]": "https://www.drupal.org/files/issues/2019-12-25/social_media_links_3102897.patch"
            },
            "drupal/ckeditor_indentblock": {
                "Update the Drupal root-relative js file path.[https://www.drupal.org/project/ckeditor_indentblock/issues/3103333]": "https://www.drupal.org/files/issues/2019-12-27/updating_file_path.patch"
            },
            "drupal/ckeditor_font": {
                "Follow core and contrib conventions for the location of plugin.js[https://www.drupal.org/project/ckeditor_font/issues/3103870]": "https://www.drupal.org/files/issues/2019-12-31/plugin-file-location-3103870-3.patch"
            },
            "drupal/entity_browser_block": {
                "Prevent PHP notice in ajax request[https://www.drupal.org/project/entity_browser_block/issues/3011868]": "https://www.drupal.org/files/issues/2018-11-06/entity_browser_block-fix-php-notice-3007227-2.patch"
            }

        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/ckeditor/plugins/{$name}": ["vendor:ckeditor-plugin"],
            "web/libraries/{$name}": [
                "type:bower-asset",
                "type:npm-asset",
                "type:drupal-library",
                "jaypan/jquery_colorpicker"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
