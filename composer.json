{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "jaypan/jquery_colorpicker",
                "version": "master",
                "dist": {
                    "url": "https://www.eyecon.ro/colorpicker/colorpicker.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/font",
                "version": "4.13.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/font/releases/font_4.13.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/lineheight",
                "version": "1.0.4",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/lineheight/releases/lineheight_1.0.4.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/indent",
                "version": "4.11.4",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/indent/releases/indent_4.11.4.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/indentblock",
                "version": "4.11.4",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/indentblock/releases/indentblock_4.11.4.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/font",
                "version": "4.13.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/font/releases/font_4.13.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
              "name": "ckeditor/dialogadvtab",
              "version": "4.8.0",
              "type": "drupal-library",
              "extra": {
                "installer-name": "ckeditor/plugins/dialogadvtab"
                },
              "dist": {
                "url": "https://download.ckeditor.com/dialogadvtab/releases/dialogadvtab_4.8.0.zip",
                "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.3",
        "acquia/lightning": "4.1.3",
        "bower-asset/jquery-simple-color": "1.2.2",
        "bower-asset/spectrum": "1.8.0",
        "ckeditor-plugin/font": "4.13.1",
        "ckeditor-plugin/indent": "4.11.4",
        "ckeditor-plugin/indentblock": "4.11.4",
        "ckeditor-plugin/lineheight": "1.0.4",
        "ckeditor/dialogadvtab": "4.8.0",
        "composer/installers": "1.9.0",
        "cweagans/composer-patches": "1.6.7",
        "drupal-composer/drupal-scaffold": "2.6.1",
        "drupal-pattern-lab/attach-library-twig-extension": "1.1.0",
        "drupal-pattern-lab/bem-twig-extension": "1.0.1",
        "drupal-pattern-lab/unified-twig-extensions": "0.1.0",
        "drupal/addtoany": "1.12.0",
