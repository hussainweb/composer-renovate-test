        "drupal/better_links": "1.0.0",
        "drupal/block_blacklist": "1.0.0",
        "drupal/blocktabs": "^1.0@beta",
        "drupal/cdn": "3.4.0",
        "drupal/ckeditor_advanced_tab": "1.0.0-rc3",
        "drupal/ckeditor_config": "2.2.0",
        "drupal/ckeditor_font": "1.0.0",
        "drupal/ckeditor_indentblock": "^1.0@beta",
        "drupal/ckeditor_lineheight": "1.1.0",
        "drupal/color_field": "2.2.0",
        "drupal/components": "1.1.0",
        "drupal/config_ignore": "2.2.0",
        "drupal/config_split": "1.4.0",
        "drupal/console": "1.9.4",
        "drupal/content_lock": "2.0.0",
        "drupal/copyright_block": "^1.0@RC",
        "drupal/core": "8.8.5",
        "drupal/cshs": "^1.0@beta",
        "drupal/devel": "2.1.0",
        "drupal/disqus": "^1.0@RC",
        "drupal/editor_advanced_link": "1.6.0",
        "drupal/entity_browser": "2.4.0",
        "drupal/entity_browser_block": "1.0.0",
        "drupal/environment_indicator": "3.7.0",
        "drupal/eu_cookie_compliance": "1.8.0",
        "drupal/facebook_pixel": "^1.0@alpha",
        "drupal/facets": "1.4.0",
        "drupal/google_tag": "1.3.0",
        "drupal/header_and_footer_scripts": "2.2.0",
        "drupal/hotjar": "1.5.0",
        "drupal/hubspot_embed": "^1.0@RC",
        "drupal/hubspot_forms": "1.6.0",
        "drupal/jquery_colorpicker": "1.4.0",
        "drupal/lightning_media": "3.16.0",
        "drupal/linkedin_insights_tag": "1.0.0",
        "drupal/migrate_file": "1.1.0",
        "drupal/migrate_plus": "5.1.0",
